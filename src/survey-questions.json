            { "type": "text", "name": "q7c", "title": "At increased risk to develop food allergy:", "titleLocation": "left", "isRequired": true, "inputType": "number", "description": "months" }
          ]
        },
        {
          "type": "checkbox",
          "name": "q8",
          "title": "When do you consider an infant at 'increased risk to develop food allergy'? (multiple choices possible)",
          "isRequired": true,
          "choices": [
            { "text": "Coexisting eczema", "value": "q8_a1" },
            { "text": "Recurrent Bronchiolitis/wheeze", "value": "q8_a2" },
            { "text": "Direct family history (i.e. parents, siblings) of any allergy, atopic dermatitis, asthma or rhinoconjuctivitis", "value": "q8_a3" }
