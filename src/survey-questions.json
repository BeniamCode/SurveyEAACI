{
  "title": "Complementary feeding: a survey on the Healthcare Professional practices",
  "showQuestionNumbers": "off",
  "pages": [
    {
      "name": "basic_information_page",
      "title": "Basic Information",
      "elements": [
        {
          "type": "radiogroup",
          "name": "q1",
          "title": "Sex",
          "isRequired": true,
          "choices": [
            { "text": "Male", "value": "q1_a1" },
            { "text": "Female", "value": "q1_a2" }
          ]
        },
        {
          "type": "dropdown",
          "name": "q2",
          "title": "Profession",
          "isRequired": true,
          "choices": [
            { "text": "Paediatrician", "value": "q2_a1" },
            { "text": "Dietitian", "value": "q2_a2" },
            { "text": "Allergist", "value": "q2_a3" },
            { "text": "Nurse", "value": "q2_a4" }
          ],
          "showOtherItem": true,
          "otherText": "Other",
          "otherPlaceholder": "Please specify"
        },
        {
          "type": "text",
          "name": "q3",
          "title": "Age (years)",
          "isRequired": true,
          "inputType": "number"
        },
        {
          "type": "text",
          "name": "q4",
          "title": "Years of practice within your specialty",
          "isRequired": true,
          "inputType": "number"
        },
        {
          "type": "radiogroup",
          "name": "q5",
          "title": "Do you have additional education in allergy?",
          "isRequired": true,
          "choices": [
            { "text": "Yes", "value": "yes" },
            { "text": "No", "value": "no" }
          ]
        },
        {
          "type": "checkbox",
          "name": "q5a",
          "title": "Please specify",
          "isRequired": true,
          "visibleIf": "{q5} = 'yes'",
          "choices": [
            { "text": "Online Course", "value": "q5a_a1" },
            { "text": "University Course", "value": "q5a_a2" },
            { "text": "MSc", "value": "q5a_a3" },
            { "text": "PhD", "value": "q5a_a4" }
          ],
          "showOtherItem": true,
          "otherText": "Other",
          "otherPlaceholder": "Please specify"
        },
        {
          "type": "text",
          "name": "q6",
          "title": "Country of work",
          "isRequired": true
        }
      ]
    },
    {
      "name": "complementary_feeding_page",
      "title": "Complementary feeding",
      "elements": [
        {
          "type": "panel",
          "name": "q7_group",
          "title": "At what age do you advise the introduction of complementary food for an infant",
          "elements": [
            { "type": "text", "name": "q7a", "title": "Breast-fed infant:", "titleLocation": "left", "isRequired": true, "inputType": "number", "description": "months" },
            { "type": "text", "name": "q7b", "title": "Formula-fed/mixed feed infant:", "titleLocation": "left", "isRequired": true, "inputType": "number", "description": "months" },
            { "type": "text", "name": "q7c", "title": "At increased risk to develop food allergy:", "titleLocation": "left", "isRequired": true, "inputType": "number", "description": "months" }
          ]
        },
        {
          "type": "checkbox",
          "name": "q8",
          "title": "When do you consider an infant at 'increased risk to develop food allergy'? (multiple choices possible)",
          "isRequired": true,
          "choices": [
            { "text": "Coexisting eczema", "value": "q8_a1" },
            { "text": "Recurrent Bronchiolitis/wheeze", "value": "q8_a2" },
            { "text": "Direct family history (i.e. parents, siblings) of any allergy, atopic dermatitis, asthma or rhinoconjuctivitis", "value": "q8_a3" }
          ],
          "showOtherItem": true,
          "otherText": "Other",
          "otherPlaceholder": "Please specify"
        },
        {
          "type": "radiogroup",
          "name": "q9a_main",
          "title": "Do you apply further measures in order to prevent food allergy in infants and young children at low risk for food allergy?",
          "isRequired": true,
          "choices": [
            { "text": "Yes", "value": "yes" },
            { "text": "No", "value": "no" }
          ]
        },
        {
          "type": "checkbox",
          "name": "q9a_elaborate",
          "title": "Please elaborate on the measures applied:",
          "isRequired": true,
          "visibleIf": "{q9a_main} = 'yes'",
          "choices": [
            { "text": "Introducing soy protein-based formula in the first 6 mo of life to prevent cow's milk allergy", "value": "q9a_a1" },
            { "text": "Partially or extensively hydrolysed formula", "value": "q9a_a2" },
            { "text": "Restricting consumption of potential food allergens during pregnancy or breastfeeding", "value": "q9a_a3" },
            { "text": "Using emollients as skin barriers", "value": "q9a_a4" },
            { "text": "Prebiotics, probiotics or synbiotics for pregnant and/or breastfeeding women and/ or infants", "value": "q9a_a5" },
            { "text": "Vitamin supplementation or fish oil supplementation during pregnancy and/or breastfeeding women and/or infants", "value": "q9a_a6" },
            { "text": "Regular cow's milk–based infant formula after the first week of life to prevent food allergy", "value": "q9a_a7" },
            { "text": "Motivate exclusive breastfeeding until 6 months", "value": "q9a_a8" }
          ],
          "showOtherItem": true,
          "otherText": "Other",
          "otherPlaceholder": "Please explain"
        },
        {
          "type": "radiogroup",
          "name": "q9b_main",
          "title": "Do you apply further measures in order to prevent food allergy in infants and young children at high risk for food allergy?",
          "isRequired": true,
          "choices": [
            { "text": "Yes", "value": "yes" },
            { "text": "No", "value": "no" }
          ]
        },
        {
          "type": "checkbox",
          "name": "q9b_elaborate",
          "title": "Please elaborate on the measures applied:",
          "isRequired": true,
          "visibleIf": "{q9b_main} = 'yes'",
          "choices": [
            { "text": "Introducing soy protein-based formula in the first 6 mo of life to prevent cow's milk allergy", "value": "q9b_a1" },
            { "text": "Partially or extensively hydrolysed formula", "value": "q9b_a2" },
            { "text": "Restricting consumption of potential food allergens during pregnancy or breastfeeding", "value": "q9b_a3" },
            { "text": "Using emollients as skin barriers", "value": "q9b_a4" },
            { "text": "Prebiotics, probiotics or synbiotics for pregnant and/or breastfeeding women and/ or infants", "value": "q9b_a5" },
            { "text": "Vitamin supplementation or fish oil supplementation during pregnancy and/or breastfeeding women and/or infants", "value": "q9b_a6" },
            { "text": "Regular cow's milk–based infant formula after the first week of life to prevent food allergy", "value": "q9b_a7" },
            { "text": "Motivate exclusive breastfeeding until 6 months", "value": "q9b_a8" }
          ],
          "showOtherItem": true,
          "otherText": "Other",
          "otherPlaceholder": "Please explain"
        },
        {
          "type": "radiogroup",
          "name": "test-conditional",
          "title": "TEST: Select Yes to see if conditional logic works",
          "choices": [
            { "text": "Yes", "value": "yes" },
            { "text": "No", "value": "no" }
          ]
        },
        {
          "type": "html",
          "name": "test-conditional-result",
          "html": "<div style='background: green; color: white; padding: 10px; margin: 10px 0;'><strong>SUCCESS!</strong> Conditional logic is working! You selected YES.</div>",
          "visibleIf": "{test-conditional} == \"yes\""
        },
        {
          "type": "radiogroup",
          "name": "q10",
          "title": "survey.questions.q10.title",
          "isRequired": true,
          "choices": [
            {
              "value": "yes",
              "text": {
                "default": "Yes",
                "en": "survey.questions.q10.choices.yes",
                "pl": "survey.questions.q10.choices.yes",
                "el": "survey.questions.q10.choices.yes",
                "zh-tw": "survey.questions.q10.choices.yes"
